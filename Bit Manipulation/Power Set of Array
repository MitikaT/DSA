https://leetcode.com/problems/subsets/

class Solution {
public:
    vector<vector<int>> subsets(vector<int>& nums) {
        int n=nums.size();
        int size=pow(2,n);
        vector<vector<int>> power_set;
        for(int i=0;i<size;i++){
            vector<int> a;
            for(int j=0;j<n;j++)
                if((i&(1<<j))!=0)
                    a.push_back(nums[j]);
            power_set.push_back(a);
        }
        return power_set;
    }
};
