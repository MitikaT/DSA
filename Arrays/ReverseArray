https://practice.geeksforgeeks.org/problems/reverse-an-array/0

#include <iostream>
using namespace std;

int main() {
	int cases,length;
	cin>>cases;
	for(int i=0;i<cases;i++){
	    cin>>length;
	    int array[length];
	    for(int j=0;j<length;j++){
	        cin>>array[j];
	    }
	    for(int k=length;k>0;k--){
	        cout<<array[k-1]<<" ";
	    }
	    cout<<endl;
	}
	return 0;
}
