https://leetcode.com/problems/max-consecutive-ones/

class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {
        int res=0,curr=0;
        for(int i=0;i<nums.size();i++){
            if(nums[i]==0)
                curr=0;
            else{
                curr++;
                res=res>curr?res:curr;
            }
        }
        return res;
    }
};
